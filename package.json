{
  "name": "n8n-nodes-plaud-unofficial",
  "version": "0.1.1",
  "description": "n8n community node for the unofficial Plaud API",
  "license": "MIT",
  "homepage": "https://github.com/leonardsellem/n8n-nodes-plaud-unofficial",
  "keywords": [
    "n8n-community-node-package",
    "plaud",
    "transcription",
    "audio",
    "voice-recorder"
  ],
  "author": {
    "name": "Leonard Sellem",
    "email": "leonard@sellem.me"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/leonardsellem/n8n-nodes-plaud-unofficial.git"
  },
  "scripts": {
    "build": "n8n-node build",
    "build:watch": "tsc --watch",
    "dev": "n8n-node dev",
    "lint": "n8n-node lint",
    "lint:fix": "n8n-node lint --fix",
    "release": "n8n-node release",
    "prepublishOnly": "n8n-node prerelease"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "strict": true,
    "credentials": [
      "dist/credentials/PlaudUnofficialApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/PlaudUnofficial/PlaudUnofficial.node.js"
    ]
  },
  "devDependencies": {
    "@n8n/node-cli": "*",
    "@types/node": "^25.0.10",
    "eslint": "9.32.0",
    "prettier": "3.6.2",
    "release-it": "^19.0.4",
    "typescript": "5.9.2"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}
